
<launch>

        <!-- this file is auto generated using preddrl_gazebo_plugin pkg -->  
        <!-- <arg name="arg_name"/> -->
        <arg name="model" default="burger"/>
        <arg name="x_pos" default="7.5"/>
        <arg name="y_pos" default="6.5"/>
        <arg name="z_pos" default="0.0"/>
        <include file="$(find gazebo_ros)/launch/empty_world.launch">
             <arg name="world_name" value="$(find preddrl_gazebo)/worlds/social_contexts.world"/>
             <arg name="use_sim_time" value="false"/>
             <arg name="gui" value="false"/>
         </include>
         
         <!-- this node spawn preddrl actors to gazebo once, then updates their pose --> 
         <!--
         <node pkg="preddrl_tracker" type="pedestrian_state_publisher.py" name="pedestrian_state_publisher"  output="screen" args="$(find preddrl_tracker)/data"/>
         -->
         <param name="robot_description" command="$(find xacro)/xacro $(find turtlebot3_description)/urdf/turtlebot3_$(arg model).urdf.xacro" />
         <!--
         <node pkg="turtlebot3_dqn"  type="odomtopath" name="odomtopath" output="screen" />
         -->
         <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-urdf -model turtlebot3_$(arg model) -x $(arg x_pos) -y $(arg y_pos) -z $(arg z_pos) -param robot_description" />

</launch>
